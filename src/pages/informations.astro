---
import LayoutWithHero from '../layouts/LayoutWithHero.astro';

import imgHero from '../assets/img/festival-02.webp';
import { Picture } from 'astro:assets';


const title = "Informations pratiques";
const subTitle = "Du 21 au 25 avril 2025";
const subTitle2 = "L'alliance du trail et de la cinématographie.";
const description = "« Ça va Trai'ler ! » accueille des festivaliers enthousiastes et cinéphiles, afin de partager ensemble leur amour du septième art et de la course à pied.";
---

<LayoutWithHero title={title} description={description} subTitle={subTitle} subTitle2={subTitle2} src={imgHero} alt="Festival en extérieur" classContent="">	
  <Fragment slot="content">
    <h1>i</h1>
  </Fragment>
</LayoutWithHero>